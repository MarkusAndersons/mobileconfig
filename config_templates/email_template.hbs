<dict>
    <key>PayloadType</key>
    <string>{{PayloadType}}</string>

    <key>PayloadVersion</key>
    <integer>{{PayloadVersion}}</integer>

    <key>PayloadIdentifier</key>
    <string>{{PayloadIdentifier}}</string>

    <key>PayloadUUID</key>
    <string>{{PayloadUUID}}</string>

    <key>PayloadDisplayName</key>
    <string>{{PayloadDisplayName}}</string>

    {{#if PayloadDescription}}
    <key>PayloadDescription</key>
    <string>{{PayloadDescription}}</string>
    {{/if}}

    {{#if PayloadOrganization}}
    <key>PayloadOrganization</key>
    <string>{{PayloadOrganization}}</string>
    {{/if}}

    {{#if EmailAccountDescription}}
    <key>EmailAccountDescription</key>
    <string>{{EmailAccountDescription}}</string>
    {{/if}}

    {{#if EmailAccountName}}
    <key>EmailAccountName</key>
    <string>{{EmailAccountName}}</string>
    {{/if}}

    <key>EmailAccountType</key>
    <string>{{EmailAccountType}}</string>

    <key>EmailAddress</key>
    <string>{{EmailAddress}}</string>

    <key>IncomingMailServerAuthentication</key>
    <string>{{IncomingMailServerAuthentication}}</string>

    <key>IncomingMailServerHostName</key>
    <string>{{IncomingMailServerHostName}}</string>

    {{#if IncomingMailServerPortNumber}}
    <key>IncomingMailServerPortNumber</key>
    <integer>{{IncomingMailServerPortNumber}}</integer>
    {{/if}}

    <key>IncomingMailServerUseSSL</key>
    {{#if IncomingMailServerUseSSL}}
    <true/>
    {{else}}
    <false/>
    {{/if}}

    <key>IncomingMailServerUsername</key>
    <string>{{IncomingMailServerUsername}}</string>

    {{#if IncomingPassword}}
    <key>IncomingPassword</key>
    <string>{{IncomingPassword}}</string>
    {{/if}}

    {{#if OutgoingPassword}}
    <key>OutgoingPassword</key>
    <string>{{OutgoingPassword}}</string>
    {{/if}}

    <key>OutgoingPasswordSameAsIncomingPassword</key>
    {{#if OutgoingPasswordSameAsIncomingPassword}}
    <true/>
    {{else}}
    <false/>
    {{/if}}

    <key>OutgoingMailServerAuthentication</key>
    <string>{{OutgoingMailServerAuthentication}}</string>

    <key>OutgoingMailServerHostName</key>
    <string>{{OutgoingMailServerHostName}}</string>

    {{#if OutgoingMailServerPortNumber}}
    <key>OutgoingMailServerPortNumber</key>
    <integer>{{OutgoingMailServerPortNumber}}</integer>
    {{/if}}

    <key>OutgoingMailServerUseSSL</key>
    {{#if OutgoingMailServerUseSSL}}
    <true/>
    {{else}}
    <false/>
    {{/if}}

    <key>OutgoingMailServerUsername</key>
    <string>{{OutgoingMailServerUsername}}</string>

    <key>PreventMove</key>
    {{#if PreventMove}}
    <true/>
    {{else}}
    <false/>
    {{/if}}

    <key>PreventAppSheet</key>
    {{#if PreventAppSheet}}
    <true/>
    {{else}}
    <false/>
    {{/if}}

    <key>allowMailDrop</key>
    {{#if allowMailDrop}}
    <true/>
    {{else}}
    <false/>
    {{/if}}

</dict>
